#include<bits/stdc++.h>
using namespace std;

int gcd(int n,int m)
{
    if(m==0) return n;
    else return gcd(m,n%m);
}

int main()
{
    int n,m,t;
    int ans,_;
    scanf("%d",&_);
    while(_--)
    {
        scanf("%d%d",&n,&m);
        ans=0;
        if(m>n) swap(n,m);
        while(1)
        {
            t=n/m;
            n-=n
        }
        printf("%d\n",ans);
    }
    return 0;
}
